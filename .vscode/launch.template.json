{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Backend",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/src/app.ts",
            "stopOnEntry": false,
            "args": [],
            "cwd": "${workspaceRoot}",
            "preLaunchTask": "npm",
            "runtimeExecutable": null,
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "console": "internalConsole",
            "sourceMaps": true,
            "outDir": "${workspaceRoot}/dist"
        }
    ]
}
